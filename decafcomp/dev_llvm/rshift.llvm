; ModuleID = 'Test'
source_filename = "DecafComp"

declare void @print_int(i32)

define i32 @main() {
func:
  %x = alloca i32
  store i32 0, i32* %x
  br label %if

if:                                               ; preds = %func
  br i1 true, label %true, label %false

true:                                             ; preds = %if
  call void @print_int(i32 7)
  call void @print_int(i32 4)
  call void @print_int(i32 1)
  br label %false

false:                                            ; preds = %true, %if
  ret i32 0
}
